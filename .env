# Backend API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_API_TIMEOUT=10000

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# App Configuration
REACT_APP_NAME=Dairy Farm Management
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Features Configuration
REACT_APP_ENABLE_OFFLINE_MODE=false
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=false
REACT_APP_DEFAULT_PAGINATION_LIMIT=10

# Theme Configuration
REACT_APP_PRIMARY_COLOR=#059669
REACT_APP_SECONDARY_COLOR=#10b981
REACT_APP_ACCENT_COLOR=#34d399